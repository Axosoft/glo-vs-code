{
  "name": "gitkraken-glo",
  "displayName": "GitKraken Boards",
  "version": "1.1.0",
  "publisher": "axosoft",
  "author": "Axosoft, LLC",
  "description": "Kanban boards for issue & task tracking.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Axosoft/glo-vs-code.git"
  },
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "issue tracker",
    "task tracker",
    "issue board",
    "project management",
    "kanban",
    "glo",
    "gitkraken"
  ],
  "icon": "icon.png",
  "main": "./out/src/extension",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "glo.open",
        "category": "Glo",
        "title": "Open GitKraken Boards"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "build": "vsce package",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vsce": "^1.30.0",
    "vscode": "^1.1.18"
  }
}
