{
  "name": "gitkraken-glo",
  "displayName": "GitKraken Glo",
  "version": "1.0.0",
  "publisher": "axosoft",
  "author": "Axosoft, LLC",
  "description": "IN CLOSED BETA. Open GitKraken Glo as a VS Code tab.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Axosoft/glo-vs-code.git"
  },
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "glo",
    "gitkraken",
    "tasks",
    "todo",
    "issue tracker"
  ],
  "icon": "icon.png",
  "main": "./out/src/extension",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.glo.open",
        "title": "Open Glo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vsce": "^1.30.0",
    "vscode": "^1.0.0"
  }
}
